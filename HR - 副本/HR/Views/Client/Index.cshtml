<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-trasitional.dtd">
@model IEnumerable<Model.config_file_first_kindModel>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <link rel="stylesheet"
          href="../../css/table.css" type="text/css">
    <script type="text/javascript"
            src="../../javascript/comm/comm.js">
    </script>
</head>
<body>
    <form method="post" action="../customiz/customizAction!findFirstKind">
        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--I级机构设置
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    @Html.ActionLink("添加", "Create")
                </td>
            </tr>
        </table>
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.first_kind_id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.first_kind_name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.first_kind_salary_id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.first_kind_sale_id)
                </th>
                <th>变更</th>
                <th>删除</th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <th>
                        @Html.DisplayFor(modelItem => item.first_kind_id)
                    </th>
                    <th>
                        @Html.DisplayFor(modelItem => item.first_kind_name)
                    </th>
                    <th>
                        @Html.DisplayFor(modelItem => item.first_kind_salary_id)
                    </th>    
                    <th>
                        @Html.DisplayFor(modelItem => item.first_kind_sale_id)
                    </th> 
                        
                    
                    <th>
                        @Html.ActionLink("变更", "Edit", new { id = item.ffk_id })
                    </th>
                    <th>
                        @Html.ActionLink("删除", "Delete", new { id = item.ffk_id })
                    </th>    
                        
                    
                </tr>
            }

        </table>
        <p>

    </form>
</body>
</html>
