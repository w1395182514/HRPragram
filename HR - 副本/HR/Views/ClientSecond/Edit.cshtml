<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-trasitional.dtd">
@model Model.config_file_second_kindModel
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <link rel="stylesheet"
          href="../../css/table.css" type="text/css">
    <script type="text/javascript"
            src="../../javascript/comm/comm.js">
    </script>
</head>
<body>
    @using (Ajax.BeginForm("Edit", new AjaxOptions() { HttpMethod = "Post" }))
    {

        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--II级机构设置--II级机构修改
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="submit" value="提交" class="BUTTON_STYLE1" id="btn1">
                    <input type="button" value="返回" class="BUTTON_STYLE1"
                           onclick="history.back();">
                </td>
            </tr>
        </table>


        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr style="display:none">
                <td width="19%" class="TD_STYLE1">
                    编号
                </td>
                <td width="81%" class="TD_STYLE2">
                    @Html.TextBoxFor(model => model.fsk_id, new { @class = "form-control", @readonly = true })
                    @Html.ValidationMessageFor(model => model.fsk_id, "", new { @class = "text-danger" })
                </td>
            </tr>
            <tr>
                <td width="19%" class="TD_STYLE1">
                    I级机构名称
                </td>
                <td width="81%" class="TD_STYLE2">
                    @Html.TextBoxFor(model => model.first_kind_name, new { @class = "form-control", @readonly = true })
                    @Html.ValidationMessageFor(e => e.first_kind_name, "", new { @class = "text-danger" })
                </td>
            </tr>
            <tr style="display:none">
                <td width="19%" class="TD_STYLE1">
                    I级机构编号
                </td>
                <td width="81%" class="TD_STYLE2">
                    @Html.TextBoxFor(model => model.first_kind_id, new { @class = "form-control", @readonly = true })
                    @Html.ValidationMessageFor(e => e.first_kind_id, "", new { @class = "text-danger" })
                </td>
            </tr>
            <tr>
                <td width="19%" class="TD_STYLE1">
                    II级机构编号
                </td>
                <td width="81%" class="TD_STYLE2">
                    @Html.TextBoxFor(model => model.second_kind_id, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.second_kind_id, "", new { @class = "text-danger" })
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    II级机构名称
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(model => model.second_kind_name, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.second_kind_name, "", new { @class = "text-danger" })
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    薪酬发放责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
                </td>
                <td class="TD_STYLE2">
                    @Html.TextAreaFor(model => model.second_salary_id, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.second_salary_id, "", new { @class = "text-danger" })
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    销售责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
                </td>
                <td class="TD_STYLE2">
                    @Html.TextAreaFor(model => model.second_sale_id, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.second_sale_id, "", new { @class = "text-danger" })
                </td>
            </tr>
        </table>
    }

</body>
</html>
