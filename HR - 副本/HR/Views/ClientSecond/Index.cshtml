<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-trasitional.dtd">
@model IEnumerable<Model.config_file_second_kindModel>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <link rel="stylesheet" href="../css/table.css"
          type="text/css">
    <script type="text/javascript"
            src="../javascript/comm/comm.js">
    </script>
</head>
<body>
@using (Ajax.BeginForm("Create", new AjaxOptions() { HttpMethod = "Post" }))
{
    <form action="../customiz/customizAction!findSecondKind" method="post">
        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--II级机构设置
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    @Html.ActionLink("添加", "Create")
                </td>
            </tr>
        </table>
        <div id="div1">

            <table width="100%" border="1" cellpadding=0 cellspacing=1
                   bordercolorlight=#848284 bordercolordark=#eeeeee
                   class="TABLE_STYLE1" id="tb">
                <tr>
                    <td width="15%" class="TD_STYLE1">
                        I级机构名称
                    </td>
                    <td width="15%" class="TD_STYLE1">
                        II级机构编号
                    </td>
                    <td width="15%" class="TD_STYLE1">
                        II级机构名称
                    </td>
                    <td width="22%" class="TD_STYLE1">
                        薪酬发放责任人编号
                    </td>
                    <td width="22%" class="TD_STYLE1">
                        销售责任人编号
                    </td>
                    <td width="5%" class="TD_STYLE1">
                        变更
                    </td>
                    <td width="5%" class="TD_STYLE1">
                        删除
                    </td>
                </tr>
                @foreach (var item in Model)
            {
                    <tr>
                       
                        <td class="TD_STYLE2">
                            @Html.DisplayFor(modelItem => item.first_kind_name)
                        </td>
                        <td class="TD_STYLE2">
                            @Html.DisplayFor(modelItem => item.second_kind_id)
                        </td>
                        <td class="TD_STYLE2">
                            @Html.DisplayFor(modelItem => item.second_kind_name)
                        </td>
                        <td class="TD_STYLE2">
                            @Html.DisplayFor(modelItem => item.second_salary_id)
                        </td>
                        <td class="TD_STYLE2">
                            @Html.DisplayFor(modelItem => item.second_sale_id)
                        </td>


                        <td class="TD_STYLE2">
                            @Html.ActionLink("变更", "Edit", new { id = item.fsk_id })
                        </td>
                        <td class="TD_STYLE2">
                            @Html.ActionLink("删除", "Delete", new { id = item.fsk_id })
                        </td>


                    </tr>
                }


            </table>
        </div>
        <br><br>
        <th colspan="7" class="pager"></th>
        <p>
    </form>
}
</body>
</html>