@model IEnumerable<Model.public_charModel>

@{
    ViewBag.Title = "Index";
}
<link href="~/css/table.css" rel="stylesheet" />

<table width="100%">
    <tr>
        <td>
            <font color="black">
                您正在做的业务是：人力资源--客户化设置--薪酬管理设置--薪酬项目设置
            </font>
        </td>
    </tr>
    <tr>
        <td align="right">
            @Html.ActionLink("添加", "Create", new { }, new { @class = "BUTTON_STYLE1" })
            
        </td>
    </tr>
</table>
<div id="div1">
    <table width="100%" border="1" cellpadding=0 cellspacing=1
           bordercolorlight=#848284 bordercolordark=#eeeeee
           class="TABLE_STYLE1">
        <tr height="20">
            <td width="80%" class="TD_STYLE1" align="center">薪酬项目名称</td>
            <td width="20%" class="TD_STYLE1" align="center">删除</td>
        </tr>

        <tr>
            <td class="TD_STYLE2">
                @foreach (var item in Model)
            {
            <tr>
                <td class="TD_STYLE2">
                    @Html.DisplayFor(modelItem => item.attribute_name)
                </td>
                <td align="center" class="TD_STYLE2">
                    @Html.ActionLink("删除", "Delete", new { id = item.pbc_id }, new { onclick = "return confirm('确认删除?')" })
                </td>
            </tr>
                }
            </table>
        </div>
